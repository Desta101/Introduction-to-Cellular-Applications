<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.DogParks">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />


    <application
        android:allowBackup="true"
        android:icon="@drawable/icondog"
        android:label="@string/app_name"
        android:roundIcon="@drawable/icondog"
        android:supportsRtl="false"
        android:theme="@style/Theme.DogParks">
        <activity android:name="com.example.DogParks.activities.Activity_Splash">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.example.DogParks.activities.Activity_MyBase" />

        <meta-data
            android:name="com.google.android.geo.API_KEY"
            android:value="@string/google_map_keys" />

        <activity android:name="com.example.DogParks.activities.Activity_Comment" />
        <activity android:name="com.example.DogParks.activities.Activity_Add" />
        <activity android:name="com.example.DogParks.activities.Activity_Info" />
        <activity android:name="com.example.DogParks.activities.Activity_List" />
        <activity android:name="com.example.DogParks.activities.Activity_Search" />
        <activity android:name="com.example.DogParks.activities.Activity_Login">
        </activity>
    </application>

</manifest>